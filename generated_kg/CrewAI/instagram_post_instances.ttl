#################################################################
# Tools
#################################################################

:tool_BrowserTools_ScrapeAndSummarize rdf:type :Tool ;
    rdfs:label "BrowserTools.scrape_and_summarize_website" ;
    rdfs:comment """
Semantic purpose: Scrape a website and produce a long summary of its content or content chunks.
Input: full URL string (e.g., https://example.com).
Outputs: textual scrapped content and summaries (used as context for agents/tasks).
Configuration: requires a Browserless API key (SERPER/BROWSERLESS keys are present in Config entries).
Note: Implementation uses HTML partitioning into chunks and summary generation per chunk; we capture purpose and required config here.
""" ;
    :hasCapability :cap_web_scraping ;
    :hasToolConfig :config_BROWSERLESS_API_KEY .

:tool_SearchTools_SearchInternet rdf:type :Tool ;
    rdfs:label "SearchTools.search_internet" ;
    rdfs:comment """
Semantic purpose: Search the Internet (generic web search) and return top organic results, title, link, snippet.
Input: query string.
Configuration: uses SERPER_API_KEY (search service) as an API key.
""" ;
    :hasCapability :cap_internet_search ;
    :hasToolConfig :config_SERPER_API_KEY .

:tool_SearchTools_SearchInstagram rdf:type :Tool ;
    rdfs:label "SearchTools.search_instagram" ;
    rdfs:comment """
Semantic purpose: Search Instagram via site-limited search (site:instagram.com) to find relevant posts.
Input: query string.
Configuration: uses SERPER_API_KEY (search service) as an API key.
""" ;
    :hasCapability :cap_instagram_search ;
    :hasToolConfig :config_SERPER_API_KEY .

#################################################################
# Configurations (Config individuals) - environment variables
#################################################################

:config_SERPER_API_KEY rdf:type :Config ;
    :configKey "SERPER_API_KEY" ;
    :configValue "REQUIRES_VALID_KEY" ;
    dct:description "API key used by SearchTools (serper.dev). Example: set in .env; present in .env.example." .

:config_BROWSERLESS_API_KEY rdf:type :Config ;
    :configKey "BROWSERLESS_API_KEY" ;
    :configValue "REQUIRES_VALID_KEY" ;
    dct:description "API key used by BrowserTools for chrome.browserless.io content endpoint. Example: set in .env; present in .env.example." .

:config_MODEL rdf:type :Config ;
    :configKey "MODEL" ;
    :configValue "openhermes" ;
    dct:description "Name of the local Ollama model to use (env var MODEL). .env.example sets MODEL='openhermes'." .

#################################################################
