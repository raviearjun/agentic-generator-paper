#################################################################
# Teams (Crews) and Workflow Patterns
#################################################################

:team_CopyCrew rdf:type :Team ;
    rdfs:label "Copy Crew" ;
    rdfs:comment "Crew responsible for producing ad copy: product_competitor_agent, strategy_planner_agent, creative_content_creator_agent." ;
    :hasAgentMember :agent_ProductCompetitor , :agent_StrategyPlanner , :agent_CreativeContentCreator ;
    :hasWorkflowPattern :wp_CopyCrew ;
    :hasSystemConfig :config_MODEL .

:wp_CopyCrew rdf:type :WorkflowPattern ;
    rdfs:label "Workflow Pattern - Copy Crew" ;
    rdfs:comment "Pattern: Analyze website -> Competitor analysis -> Campaign strategy -> Write ad copy" ;
    :hasWorkflowStep :step_Copy_1 , :step_Copy_2 , :step_Copy_3 , :step_Copy_4 .

:step_Copy_1 rdf:type :WorkflowStep ;
    rdfs:label "Step 1 - Website analysis" ;
    :stepOrder 1 ;
    :hasAssociatedTask :task_ProductAnalysis ;
    :relatedStep :step_Copy_2 .

:step_Copy_2 rdf:type :WorkflowStep ;
    rdfs:label "Step 2 - Competitor analysis" ;
    :stepOrder 2 ;
    :hasAssociatedTask :task_CompetitorAnalysis ;
    :relatedStep :step_Copy_3 .

:step_Copy_3 rdf:type :WorkflowStep ;
    rdfs:label "Step 3 - Campaign development" ;
    :stepOrder 3 ;
    :hasAssociatedTask :task_CampaignDevelopment ;
    :relatedStep :step_Copy_4 .

:step_Copy_4 rdf:type :WorkflowStep ;
    rdfs:label "Step 4 - Instagram ad copy" ;
    :stepOrder 4 ;
    :hasAssociatedTask :task_InstagramAdCopy .

:team_ImageCrew rdf:type :Team ;
    rdfs:label "Image Crew" ;
    rdfs:comment "Crew responsible for image concepts: senior_photographer_agent, chief_creative_director_agent." ;
    :hasAgentMember :agent_SeniorPhotographer , :agent_ChiefCreativeDirector ;
    :hasWorkflowPattern :wp_ImageCrew ;
    :hasSystemConfig :config_MODEL , :config_BROWSERLESS_API_KEY , :config_SERPER_API_KEY .

:wp_ImageCrew rdf:type :WorkflowPattern ;
    rdfs:label "Workflow Pattern - Image Crew" ;
    rdfs:comment "Pattern: Take photograph concepts based on ad copy -> Review/approve or delegate follow-ups." ;
    :hasWorkflowStep :step_Image_1 , :step_Image_2 .

:step_Image_1 rdf:type :WorkflowStep ;
    rdfs:label "Step 1 - Take photograph" ;
    :stepOrder 1 ;
    :hasAssociatedTask :task_TakePhotograph ;
    :relatedStep :step_Image_2 .

:step_Image_2 rdf:type :WorkflowStep ;
    rdfs:label "Step 2 - Review photo" ;
    :stepOrder 2 ;
    :hasAssociatedTask :task_ReviewPhoto .

#################################################################
# Goals / Objectives
#################################################################

:goal_MarketingCampaign rdf:type :Goal ;
    rdfs:label "Marketing Campaign Objective" ;
    dct:description "Overarching objective: produce Instagram post copy and photograph concepts that highlight the product, its unique selling points, and create a high-impact campaign." .

:objective_CopyCrew rdf:type :Objective ;
    rdfs:label "Copy Crew Objective" ;
    dct:description "Generate analysis-informed ad copy and campaign strategy to drive engagement." ;
    :contributesToGoal :goal_MarketingCampaign .

:objective_ImageCrew rdf:type :Objective ;
    rdfs:label "Image Crew Objective" ;
    dct:description "Create photograph concepts aligned with ad copy and campaign to produce visuals for Instagram." ;
    :contributesToGoal :goal_MarketingCampaign .

# Link agents / teams to goals / objectives
:agent_ProductCompetitor :hasGoal :goal_MarketingCampaign ;
    :hasObjective :objective_CopyCrew .

:agent_StrategyPlanner :hasGoal :goal_MarketingCampaign ;
    :hasObjective :objective_CopyCrew .

:agent_CreativeContentCreator :hasGoal :goal_MarketingCampaign ;
    :hasObjective :objective_CopyCrew .

:agent_SeniorPhotographer :hasGoal :goal_MarketingCampaign ;
    :hasObjective :objective_ImageCrew .

:agent_ChiefCreativeDirector :hasGoal :goal_MarketingCampaign ;
    :hasObjective :objective_ImageCrew .

:team_CopyCrew :hasTeamGoal :goal_MarketingCampaign .
:team_ImageCrew :hasTeamGoal :goal_MarketingCampaign .

