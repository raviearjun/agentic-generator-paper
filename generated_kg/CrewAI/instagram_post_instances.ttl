    :performedByAgent :agent_ChiefCreativeDirector ;
    :producedResource :resource_ApprovedImageDescriptions ;
    :requiresResource :resource_ImageDescriptions , :resource_ProductAnalysisReport ;
    :requiresResource :config_MODEL .

