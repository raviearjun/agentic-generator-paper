### ================================
### Resources
### ================================

ex:assistant
    a :Agent ;
    :agentID "chess_game_assistant" ;
    :agentRole "Assistant Agent" ;
    dcterms:title "chess_game_assistant Assistant" ;
    dcterms:description "You are a helpful AI assistant for chess_game" ;
    :hasGoal ex:goal_assistant ;
    :hasTask ex:task_assistant .


ex:goal_assistant
    a :Goal ;
    dcterms:title "Goal for chess_game_assistant" ;
    dcterms:description "Automatically generated goal for chess_game_assistant" .


ex:task_assistant
    a :Task ;
    dcterms:title "Task for chess_game_assistant" ;
    dcterms:description "Automatically generated task for chess_game_assistant" ;
    :taskExpectedOutput "Automatically generated expected output for chess_game_assistant" .


ex:userproxy
    a :Agent ;
    :agentID "user" ;
    :agentRole "User Proxy Agent" ;
    dcterms:title "User Proxy" ;
    dcterms:description "Human input proxy with NEVER mode" ;
    :hasGoal ex:goal_userproxy ;
    :hasTask ex:task_userproxy .


ex:goal_userproxy
    a :Goal ;
    dcterms:title "Goal for user" ;
    dcterms:description "Automatically generated goal for user" .


ex:task_userproxy
    a :Task ;
    dcterms:title "Task for user" ;
    dcterms:description "Automatically generated task for user" ;
    :taskExpectedOutput "Automatically generated expected output for user" .


ex:chess_game_assistant_workflow
    a :WorkflowPattern ;
    dcterms:title "chess_game_assistant Workflow" .


ex:DatatypeProperty_systemmessage
    a agento:DatatypeProperty ;
    agento:domain agento:Agent ;
    agento:justification "Ditemukan secara konsisten di pattern AutoGen." ;
    agento:name "systemMessage" ;
    agento:range xsd:string .


ex:DatatypeProperty_humaninputmode
    a agento:DatatypeProperty ;
    agento:domain agento:Agent ;
    agento:justification "Relevan untuk agen proxy di AutoGen." ;
    agento:name "humanInputMode" ;
    agento:range xsd:string .


ex:DatatypeProperty_vendorclass
    a agento:DatatypeProperty ;
    agento:domain agento:Agent ;
    agento:justification "Membantu interoperabilitas lintas framework." ;
    agento:name "vendorClass" ;
    agento:range xsd:string .
