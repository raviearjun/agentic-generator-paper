@prefix : <http://www.w3id.org/agentic-ai/onto#> .
@prefix agento: <http://www.w3id.org/agentic-ai/onto#> .
@prefix ex: <http://www.w3id.org/agentic-ai/instances#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

### ================================
### Resources
### ================================

ex:assistant
    a :Agent ;
    :agentID "planning_agent_assistant" ;
    :agentRole "Assistant Agent" ;
    dcterms:title "planning_agent_assistant Assistant" ;
    dcterms:description "You are a helpful AI assistant for planning_agent" ;
    :hasGoal ex:goal_assistant ;
    :hasTask ex:task_assistant .


ex:goal_assistant
    a :Goal ;
    dcterms:title "Goal for planning_agent_assistant" ;
    dcterms:description "Automatically generated goal for planning_agent_assistant" .


ex:task_assistant
    a :Task ;
    dcterms:title "Task for planning_agent_assistant" ;
    dcterms:description "Automatically generated task for planning_agent_assistant" ;
    :taskExpectedOutput "Automatically generated expected output for planning_agent_assistant" .


ex:userproxy
    a :Agent ;
    :agentID "user" ;
    :agentRole "User Proxy Agent" ;
    dcterms:title "User Proxy" ;
    dcterms:description "Human input proxy with NEVER mode" ;
    :hasGoal ex:goal_userproxy ;
    :hasTask ex:task_userproxy .


ex:goal_userproxy
    a :Goal ;
    dcterms:title "Goal for user" ;
    dcterms:description "Automatically generated goal for user" .


ex:task_userproxy
    a :Task ;
    dcterms:title "Task for user" ;
    dcterms:description "Automatically generated task for user" ;
    :taskExpectedOutput "Automatically generated expected output for user" .


ex:planning_agent_assistant_workflow
    a :WorkflowPattern ;
    dcterms:title "planning_agent_assistant Workflow" ;
    dcterms:description "Pattern ini mendefinisikan dua agent: satu AssistantAgent yang berperan sebagai AI assistant, dan satu UserProxyAgent yang mewakili user dengan mode input manusia NEVER. Ini adalah struktur AutoGen dasar tanpa workflow, tool, atau task." .


ex:DatatypeProperty_systemmessage
    a agento:DatatypeProperty ;
    agento:domain agento:Agent ;
    agento:justification "Ada di semua AutoGen AssistantAgent." ;
    agento:name "systemMessage" ;
    agento:range xsd:string .


ex:DatatypeProperty_humaninputmode
    a agento:DatatypeProperty ;
    agento:domain agento:Agent ;
    agento:justification "Atribut utama UserProxyAgent." ;
    agento:name "humanInputMode" ;
    agento:range xsd:string .
