### ================================
### Resources
### ================================

ex:assistant
    a :Agent ;
    :agentID "code_execution_assistant" ;
    :agentRole "Assistant Agent" ;
    dcterms:title "code_execution_assistant Assistant" ;
    dcterms:description "You are a helpful AI assistant for code_execution" ;
    :hasGoal ex:goal_assistant ;
    :hasTask ex:task_assistant .


ex:goal_assistant
    a :Goal ;
    dcterms:title "Goal for code_execution_assistant" ;
    dcterms:description "Automatically generated goal for code_execution_assistant" .


ex:task_assistant
    a :Task ;
    dcterms:title "Task for code_execution_assistant" ;
    dcterms:description "Automatically generated task for code_execution_assistant" ;
    :taskExpectedOutput "Automatically generated expected output for code_execution_assistant" .


ex:userproxy
    a :Agent ;
    :agentID "user" ;
    :agentRole "User Proxy Agent" ;
    dcterms:title "User Proxy" ;
    dcterms:description "Human input proxy with NEVER mode" ;
    :hasGoal ex:goal_userproxy ;
    :hasTask ex:task_userproxy .


ex:goal_userproxy
    a :Goal ;
    dcterms:title "Goal for user" ;
    dcterms:description "Automatically generated goal for user" .


ex:task_userproxy
    a :Task ;
    dcterms:title "Task for user" ;
    dcterms:description "Automatically generated task for user" ;
    :taskExpectedOutput "Automatically generated expected output for user" .


ex:code_execution_assistant_workflow
    a :WorkflowPattern ;
    dcterms:title "code_execution_assistant Workflow" ;
    dcterms:description "Pattern ini mendefinisikan dua agen — AssistantAgent dan UserProxyAgent — untuk skenario eksekusi kode. Struktur pattern hampir identik dengan chess_game.py, hanya berbeda pada konteks sistem pesan (code execution)." .


ex:DatatypeProperty_systemmessage
    a agento:DatatypeProperty ;
    agento:domain agento:Agent ;
    agento:justification "Ditemukan secara konsisten di pattern AutoGen." ;
    agento:name "systemMessage" ;
    agento:range xsd:string .


ex:DatatypeProperty_humaninputmode
    a agento:DatatypeProperty ;
    agento:domain agento:Agent ;
    agento:justification "Diperlukan untuk AutoGen Proxy Agent" ;
    agento:name "humanInputMode" ;
    agento:range xsd:string .


ex:DatatypeProperty_vendorclass
    a agento:DatatypeProperty ;
    agento:domain agento:Agent ;
    agento:justification "Untuk kompatibilitas lintas framework." ;
    agento:name "vendorClass" ;
    agento:range xsd:string .
