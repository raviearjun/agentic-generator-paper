@prefix : <http://www.w3id.org/agentic-ai/onto#> .
@prefix agento: <http://www.w3id.org/agentic-ai/onto#> .
@prefix ex: <http://www.w3id.org/agentic-ai/instances#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

### ================================
### Resources
### ================================

ex:assistant
    a :Agent ;
    :agentID "group_chat_assistant" ;
    :agentRole "Assistant Agent" ;
    dcterms:title "group_chat_assistant Assistant" ;
    dcterms:description "You are a helpful AI assistant for group_chat" ;
    :hasGoal ex:goal_assistant ;
    :hasTask ex:task_assistant .


ex:goal_assistant
    a :Goal ;
    dcterms:title "Goal for group_chat_assistant" ;
    dcterms:description "Automatically generated goal for group_chat_assistant" .


ex:task_assistant
    a :Task ;
    dcterms:title "Task for group_chat_assistant" ;
    dcterms:description "Automatically generated task for group_chat_assistant" ;
    :taskExpectedOutput "Automatically generated expected output for group_chat_assistant" .


ex:userproxy
    a :Agent ;
    :agentID "user" ;
    :agentRole "User Proxy Agent" ;
    dcterms:title "User Proxy" ;
    dcterms:description "Human input proxy with NEVER mode" ;
    :hasGoal ex:goal_userproxy ;
    :hasTask ex:task_userproxy .


ex:goal_userproxy
    a :Goal ;
    dcterms:title "Goal for user" ;
    dcterms:description "Automatically generated goal for user" .


ex:task_userproxy
    a :Task ;
    dcterms:title "Task for user" ;
    dcterms:description "Automatically generated task for user" ;
    :taskExpectedOutput "Automatically generated expected output for user" .


ex:group_chat_assistant_workflow
    a :WorkflowPattern ;
    dcterms:title "group_chat_assistant Workflow" ;
    dcterms:description "Pattern ini mendefinisikan dua agent (AssistantAgent dan UserProxyAgent) dalam konteks komunikasi berbasis grup (group chat). Walau strukturnya serupa dengan pattern lain, konteks “group_chat” mengimplikasikan adanya pertukaran pesan (message exchange) antar-agent yang berpartisipasi dalam satu percakapan." .


ex:DatatypeProperty_receivemessage
    a agento:DatatypeProperty ;
    agento:domain agento:Agent ;
    agento:justification "Esensial untuk model komunikasi dua arah." ;
    agento:name "receiveMessage" ;
    agento:range xsd:string .


ex:conversation
    a owl:Class ;
    dcterms:title "conversation" ;
    dcterms:description "Aktivitas percakapan antar-agent." .


ex:message
    a owl:Class ;
    dcterms:title "message" ;
    dcterms:description "Representasi pesan yang dikirim dalam percakapan." .
